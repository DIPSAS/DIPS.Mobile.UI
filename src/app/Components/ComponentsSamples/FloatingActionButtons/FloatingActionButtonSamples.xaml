<?xml version="1.0" encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dui="http://dips.com/mobile.ui"
             xmlns:floatingActionButtons="clr-namespace:Components.ComponentsSamples.FloatingActionButtons"
             x:Class="Components.ComponentsSamples.FloatingActionButtons.FloatingActionButtonSamples">
    
    <dui:ContentPage.BindingContext>
        <floatingActionButtons:FloatingActionButtonSamplesViewModel />
    </dui:ContentPage.BindingContext>
    
    <dui:VerticalStackLayout>
        <dui:Button Text="Floating Navigation Button" Command="{Binding NavigateToFloatingNavigationButtonSamples}" />
    </dui:VerticalStackLayout>
    
</dui:ContentPage>