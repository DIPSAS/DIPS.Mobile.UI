<?xml
    version="1.0"
    encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                 xmlns:dui="http://dips.com/mobile.ui"
                 xmlns:playground="clr-namespace:Playground"
                 x:Class="Playground.MainPage"
                 Title="Welcome to Playground">
    <dui:ScrollView>
        <VerticalStackLayout>
            <dui:NavigationListItem Title="Vetle"
                                    >
                <dui:NavigationListItem.GestureRecognizers>
                    <TapGestureRecognizer Tapped="GoToVetle"></TapGestureRecognizer>
                </dui:NavigationListItem.GestureRecognizers>
                
            </dui:NavigationListItem>
            <dui:NavigationListItem Title="Håvard"
                                    Tapped="GoToHåvard" />
            <dui:NavigationListItem Title="Sander"
                                    Tapped="GoToSander" />
            <dui:NavigationListItem Title="Eirik"
                                    Tapped="GoToEirik" />
            <dui:NavigationListItem Title="CollectionViewTests"
                                    Tapped="GoToCollectionViewTests" />
            <dui:Button Text="Tap me"
                        Clicked="TapMe" />
        </VerticalStackLayout>
    </dui:ScrollView>
</dui:ContentPage>