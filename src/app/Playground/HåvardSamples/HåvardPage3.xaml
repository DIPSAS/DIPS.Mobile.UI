<?xml
    version="1.0"
    encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                 xmlns:dui="http://dips.com/mobile.ui"
                 xmlns:ui="clr-namespace:DIPS.Mobile.UI;assembly=DIPS.Mobile.UI"
                 xmlns:håvardSamples="clr-namespace:Playground.HåvardSamples"
                 x:Class="Playground.HåvardSamples.HåvardPage3"
                 ShouldGarbageCollectAndLogWhenNavigatedTo="True"
                 Unloaded="HåvardPage3_OnUnloaded">
    <dui:ContentPage.BindingContext>
        <håvardSamples:HåvardPageViewModel />
    </dui:ContentPage.BindingContext>
    <dui:Button Text="Open context menu"
                dui:ContextMenuEffect.Mode="LongPressed">
        <dui:ContextMenuEffect.Menu>
            <dui:ContextMenu>
                <dui:ContextMenuItem Title="Testing"/>
            </dui:ContextMenu>
        </dui:ContextMenuEffect.Menu>
    </dui:Button>
</dui:ContentPage>