<?xml version="1.0" encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dui="http://dips.com/mobile.ui"
             xmlns:vetleSamples="clr-namespace:Playground.VetleSamples"
             xmlns:modal="clr-namespace:DIPS.Mobile.UI.Effects.Modal;assembly=DIPS.Mobile.UI"
             xmlns:extensions="clr-namespace:DIPS.Mobile.UI.Components.ListItems.Extensions;assembly=DIPS.Mobile.UI"
             x:Class="Playground.VetleSamples.VetleTestPage2"
             x:DataType="{x:Type vetleSamples:VetleTestPage1ViewModel}"
             Title="Hoho"
             modal:Modal.HasNavBar="True">
    <dui:ContentPage.BindingContext>
        <vetleSamples:VetleTestPage1ViewModel />
    </dui:ContentPage.BindingContext>
    
    <dui:VerticalStackLayout>
        <extensions:NavigationListItem Tapped="NavigationListItem_OnTapped"> </extensions:NavigationListItem>
        
        <Grid WidthRequest="100"
                HeightRequest="120"
                BackgroundColor="Black"
                dui:Touch.Command="{Binding Test}"
                dui:Touch.LongPressCommand="{Binding Test}" />
    </dui:VerticalStackLayout>
</dui:ContentPage>