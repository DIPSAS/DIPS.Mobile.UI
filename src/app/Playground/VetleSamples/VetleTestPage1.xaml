<?xml version="1.0" encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dui="http://dips.com/mobile.ui"
             xmlns:vetleSamples="clr-namespace:Playground.VetleSamples"
             xmlns:dividers="clr-namespace:DIPS.Mobile.UI.Components.Dividers;assembly=DIPS.Mobile.UI"
             xmlns:modal="clr-namespace:DIPS.Mobile.UI.Effects.Modal;assembly=DIPS.Mobile.UI"
             xmlns:extensions="clr-namespace:DIPS.Mobile.UI.Components.ListItems.Extensions;assembly=DIPS.Mobile.UI"
             x:Class="Playground.VetleSamples.VetleTestPage1"
             x:Name="CodeBehind"
             Title="Hei"
             Padding="30, 30">
    
    <dui:ContentPage.BindingContext>
        <vetleSamples:VetlePageViewModel />
    </dui:ContentPage.BindingContext>
    
    <dui:ContentPage.ToolbarItems>
        <ToolbarItem Text="Hei" />
    </dui:ContentPage.ToolbarItems>
    
    <!--<dui:ContentPage.BindingContext>
        <vetleSamples:VetleTestPage1ViewModel />
    </dui:ContentPage.BindingContext>-->
    
    <dui:VerticalStackLayout x:Name="Layout">
        <dui:LoadableListItem Title="Hei" 
                              IsBusy="{Binding IsProgressing}" />

       
                    
    </dui:VerticalStackLayout>
</dui:ContentPage>