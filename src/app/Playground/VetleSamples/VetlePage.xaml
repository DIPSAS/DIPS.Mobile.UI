<?xml version="1.0" encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                 xmlns:vetleSamples="clr-namespace:Playground.VetleSamples"
                 xmlns:dui="http://dips.com/mobile.ui"
                 x:Class="Playground.VetleSamples.VetlePage"
                 x:DataType="{x:Type vetleSamples:VetlePageViewModel}"
                 Padding="20"
                 x:Name="This">

    <dui:ContentPage.BindingContext>
        <vetleSamples:VetlePageViewModel />
    </dui:ContentPage.BindingContext>
   
    <Grid>
    <stateView:StateView CurrentState="{Binding CurrentState}"
                         StateViewModel="{Binding StateViewModel}">
        
        <dui:Label Text="Hello this is me!" />
        
    </stateView:StateView>
    <dui:Button HorizontalOptions="End" Text="e" Command="{Binding SaveSuccess}"></dui:Button>
    </Grid>
</dui:ContentPage>