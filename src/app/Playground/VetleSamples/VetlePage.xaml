<?xml
    version="1.0"
    encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
                 xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                 xmlns:vetleSamples="clr-namespace:Playground.VetleSamples"
                 xmlns:dui="http://dips.com/mobile.ui"
                 xmlns:converters="clr-namespace:Arena.Mobile.Domains.MunicipalCare.Shared.Converters"
                 x:Class="Playground.VetleSamples.VetlePage"
                 x:Name="This"
                 HideSoftInputOnTapped="True">

    <dui:ContentPage.BindingContext>
        <vetleSamples:VetlePageViewModel />
    </dui:ContentPage.BindingContext>

    <!--<dui:NavigationListItem />-->
    <dui:ListItem VerticalOptions="Start"
                  Title="Tore"
                  Subtitle="Geir"
                  Tapped="ListItem_OnTapped">
        
        <dui:ListItem.TitleOptions>
            <dui:TitleOptions Width="30"></dui:TitleOptions>
        </dui:ListItem.TitleOptions>
        
        <dui:Image Source="{dui:Icons chevron_right_line}" />
        
        <dui:ListItem.UnderlyingContent>
            <dui:Label Text="Stine"></dui:Label>
        </dui:ListItem.UnderlyingContent>
        
    </dui:ListItem>



</dui:ContentPage>