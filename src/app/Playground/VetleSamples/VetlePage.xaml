<?xml version="1.0" encoding="utf-8"?>

<dui:ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:dui="http://dips.com/mobile.ui"
             xmlns:vetleSamples="clr-namespace:Playground.VetleSamples"
             xmlns:inLineContent="clr-namespace:DIPS.Mobile.UI.Components.ListItems.Options.InLineContent;assembly=DIPS.Mobile.UI"
             xmlns:extensions="clr-namespace:DIPS.Mobile.UI.Components.ListItems.Extensions;assembly=DIPS.Mobile.UI"
             x:Class="Playground.VetleSamples.VetlePage"
             x:DataType="{x:Type vetleSamples:VetlePageViewModel}"
             >
    
    <dui:ContentPage.BindingContext>
        <vetleSamples:VetlePageViewModel />
    </dui:ContentPage.BindingContext>
    
    
    
    <dui:CollectionView ItemsSource="{Binding TestStrings}"
                        ItemSpacing="{dui:Sizes size_10}">
            
        
        <dui:CollectionView.ItemTemplate>
            <DataTemplate>
                <dui:ListItem  Title="{Binding .}" />
            </DataTemplate>
        </dui:CollectionView.ItemTemplate>
        
    </dui:CollectionView>
    

</dui:ContentPage>